<h1>Listing attacks</h1>

<table >
  <thead>
    <tr>
      <th>Vector</th>
      <th>Ip</th>
      <th>Time</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attacks.each do |attack| %>
      <tr >
        <td style="border: 1px solid black;"><%= attack.vector %></td>
        <td style="border: 1px solid black;"><%= attack.ip %></td>
        <td style="border: 1px solid black;"><%= attack.updated_at %></td>
        <td style="border: 1px solid black;"><%= link_to 'Show', attack %></td>
        <td style="border: 1px solid black;"><%= link_to 'Edit', edit_attack_path(attack) %></td>
        <td style="border: 1px solid black;"><%= link_to 'Destroy', attack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

